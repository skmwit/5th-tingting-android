<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="oval" >

            <size
                android:width="@dimen/current_matching_team_item_width"
                android:height="@dimen/current_matching_team_item_width"/>

            <stroke
                android:width="1dp"
                android:color="@color/gray" />
            <solid android:color="@color/darkGray"/>
        </shape>
    </item>
</layer-list>
